{{#each albums}}

<div class="card border-primary" style="width:250px">
    <img class="card-img-top" src={{images.0.url}} style="width:150px" alt="no image available">
    <div class="card-body">
        <h4 class="card-title">{{name}}</h4>
        <a href="/tracks/{{id}}" class="btn btn-primary">Listen tracks</a>
    </div>
</div>


{{/each}}